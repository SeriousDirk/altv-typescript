{
    "name": "altv-quickstart-ts",
    "version": "4.1.0",
    "description": "The Fastest Typescript Repo for alt:V",
    "main": "",
    "type": "module",
    "scripts": {
        "clean": "rimraf ./resources",
        "build": "node bundle.cjs",
        "update": "altv-pkg d release",
        "windows": "npm run build && altv-server.exe",
        "linux": "npm run build && ./start.sh",
        "windev": "nodemon --watch \"./src/**\" --ext \"ts\" --exec \"npm run build && altv-server.exe\"",
        "dev": "npm run windev",
        "formatting": "prettier --write .",
        "reinstall": "rimraf node_modules && npm install"
    },
    "author": "stuyk x botis",
    "license": "ISC",
    "devDependencies": {
        "@altv/types-client": "^2.1.4",
        "@altv/types-natives": "^1.4.4",
        "@altv/types-server": "^2.3.1",
        "@altv/types-shared": "^1.2.3",
        "@altv/types-webview": "^1.0.2",
        "@altv/types-worker": "^1.0.7",
        "@swc/cli": "^0.1.55",
        "@swc/core": "^1.2.207",
        "@types/node": "^17.0.45",
        "@types/react": "^18.0.14",
        "altv-pkg": "^1.2.0",
        "css-loader": "^6.7.1",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.6.1",
        "nodemon": "^2.0.7",
        "prettier": "2.7.1",
        "react": "^18.2.0",
        "rimraf": "^3.0.2",
        "sass": "^1.53.0",
        "sass-loader": "^13.0.2",
        "swc-loader": "^0.2.3",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.8.1",
        "tsup": "^4.11.2",
        "typescript": "^4.7.4",
        "vue-loader": "^17.0.0",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0"
    },
    "dependencies": {
        "cfg-reader": "^3.0.2",
        "discord-webhook-node": "^1.1.8",
        "mysql2": "^2.3.3",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.6",
        "vue": "^3.2.36"
    }
}
